   "source": [
    "df=label_unknown.merge(rater_goodness,how='left',on='turk')\n",
    "df=df[['turk','url','category_label','odds']]\n",
    "\n",
    "df.dropna(inplace=True)\n",
    "df=df.drop_duplicates(['url','category_label','turk'])\n",
    "\n",
    "\n",
    "\n",
    "df_tbu=rater_goodness[rater_goodness['the number of ratings']>=1]\n",
    "mask_1= (df_tbu['the number of ratings']> df_tbu['the number of ratings'].quantile(0.25))\n",
    "\n",
    "reliable_turks=list(df_tbu[mask_1].index)\n",
    "\n",
    "def reliable(e):\n",
    "    if e in reliable_turks:\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "\n",
    "\n",
    "df_reliable=df.loc[df['turk'].map(reliable),]\n",
    "\n",
    "label_unknown_odds=df_reliable.groupby(['url','category_label'])['odds'].prod().to_frame('overall_odds')\n",
    "\n",
    "\n",
    "\n",
    "temp=label_unknown_odds.unstack('category_label').fillna(0)['overall_odds']\n",
    "\n",
    "temp['top category']=temp.idxmax(axis=1)\n",
    "temp['top odds']=temp.max(axis=1)\n",
    "result_25=temp[['top category','top odds']]\n",
    "result_25.head(2)"
   ]
