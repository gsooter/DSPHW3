   "source": [
    "df=label_on_gold.copy()\n",
    "\n",
    "df['correctness']=(df['category_gold']==df['category_label']).astype(int)\n",
    "\n",
    "rater_goodness=df.groupby('turk')['correctness'].agg(['count','mean'])\n",
    "\n",
    "rater_goodness.rename(columns={'count': 'the number of ratings','mean':'average correctness of ratings'},inplace=True)\n",
    "rater_goodness#.sort_values('average correctness of ratings',ascending=False)\n"
   ]
  },
