   "source": [
    "df=labels.merge(gold,how='left',left_on='url',right_on='url',suffixes=['_label','_gold'])\n",
    "\n",
    "label_on_gold=df[df['category_gold'].notna()]\n",
    "label_unknown=df[df['category_gold'].isna()]\n",
    "\n",
    "df"
   ]
